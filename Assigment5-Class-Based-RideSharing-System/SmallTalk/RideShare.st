Object subclass: #Driver	instanceVariableNames: 'driverID name rating assignedRides'	classVariableNames: ''	package: 'RideShare'!!Driver methodsFor: 'printing' stamp: 'HoaiNamTran 11/15/2025 16:37'!printRides    Transcript show: self getInfo; cr.    assignedRides do: [:r |        Transcript            show: '  - ' , r rideDetails , ' | Fare: $' , r fare asString;            cr ].! !!Driver methodsFor: 'adding' stamp: 'HoaiNamTran 11/15/2025 16:36'!addRide: aRide    assignedRides add: aRide.! !!Driver methodsFor: 'initialization' stamp: 'HoaiNamTran 11/15/2025 16:36'!initializeWithId: anId name: aName rating: aRating    driverID := anId.    name := aName.    rating := aRating.    assignedRides := OrderedCollection new.! !!Driver methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:37'!getInfo    ^ 'Driver[' , driverID , '] ' , name ,      ' | Rating: ' , rating asString ,      ' | Rides: ' , assignedRides size asString.! !Object subclass: #Ride	instanceVariableNames: 'rideID pickupLocation dropoffLocation distance'	classVariableNames: ''	package: 'RideShare'!!Ride methodsFor: 'initialization' stamp: 'HoaiNamTran 11/15/2025 16:34'!initializeWithId: anId pickup: pickup drop: dropoff miles: miles    rideID := anId.    pickupLocation := pickup.    dropoffLocation := dropoff.    distance := miles.! !!Ride methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:34'!fare    self subclassResponsibility.! !!Ride methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:34'!rideDetails    ^ 'Ride[' , rideID , '] ' , pickupLocation , ' -> ' , dropoffLocation ,      ' (' , distance asString , ' mi)'.! !Ride subclass: #PremiumRide	instanceVariableNames: ''	classVariableNames: ''	package: 'RideShare'!!PremiumRide methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:35'!fare    ^ 5.0 + (distance * 3.0).! !!PremiumRide methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:36'!rideDetails    ^ 'Premium  ' , super rideDetails.! !Object subclass: #Rider	instanceVariableNames: 'riderID name requestedRides'	classVariableNames: ''	package: 'RideShare'!!Rider methodsFor: 'initialization' stamp: 'HoaiNamTran 11/15/2025 16:37'!initializeWithId: anId name: aName    riderID := anId.    name := aName.    requestedRides := OrderedCollection new.! !!Rider methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:38'!requestRide: aRide    requestedRides add: aRide.! !!Rider methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:38'!viewRides    Transcript        show: 'Rider[' , riderID , '] ' , name ,              ' | Requested rides: ' , requestedRides size asString;        cr.    requestedRides do: [:r |        Transcript            show: '  - ' , r rideDetails , ' | Fare: $' , r fare asString;            cr ].! !Ride subclass: #StandardRide	instanceVariableNames: ''	classVariableNames: ''	package: 'RideShare'!!StandardRide methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:35'!fare    | f |    f := distance * 1.5.    ^ (f < 2.0)        ifTrue: [ 2.0 ]        ifFalse: [ f ].! !!StandardRide methodsFor: 'as yet unclassified' stamp: 'HoaiNamTran 11/15/2025 16:35'!rideDetails    ^ 'Standard ' , super rideDetails.! !